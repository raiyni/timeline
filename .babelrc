{
    "presets": [
      ["@babel/preset-env",
        {
          "modules": false,
          "targets": {
            "ie": "11"
          }
        }],
      ["@babel/typescript", { "jsxPragma": "h" }]
    ], 
    "plugins": [
        ["@babel/plugin-transform-react-jsx", {
          "pragma": "h",
          "pragmaFrag": "Fragment"
        }]
      ]
  }